{
    "name": "AdBlocker",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Block annoying ads on webpages",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "browser_action": {
      "default_title": "AdBlocker",
      "default_popup": "popup.html"
    }
  }
  